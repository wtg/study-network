<div class="panel panel-default">
  <div class="panel-heading">
    <h3> Editing <%= link_to @course.title, @course %> </h3>
  </div>
  <div class="panel-body">
    <%= form_for :course, url: course_path(@course), method: :patch do |f| %>
    <br>
    <p>
      <%= f.label :title %>
      <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :crn %>
      <%= f.text_field :crn %>
    </p>
    <p>
      <%= f.label :abrev_name %>
      <%= f.text_field :abrev_name %>
    </p>
    <p>
      <%= f.label :level %>
      <%= f.text_field :level %>
    </p>
    <p>
      <%= f.label :section %>
      <%= f.text_field :section %>
    </p>
    <p>
      <%= f.submit :class => 'btn btn-primary btn-lg' %>
    </p>
    <% end %>
  </div>
</div>
